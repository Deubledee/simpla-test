{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view1.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "favicon.ico",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "name": "bundled-custom",
      "bundle": {
        "stripComments": true,
        "inlineCss": false,
        "sourcemaps": true
      },
      "addServiceWorker": true,
      "insertPrefetchLinks": true,
      "swPrecacheConfig": "sw-precache-config.js",
      "addPushManifest": true,
      "js": {
        "compile": false,
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    },
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}